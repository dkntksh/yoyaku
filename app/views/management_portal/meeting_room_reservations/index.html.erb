<h1>予約一覧</h1>
<div class="flash-notifications">
  <%= flash[:notice] %>
</div>

<%= paginate @meeting_room_reservations %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_room_reservations.each do |meeting_room_reservation| %>
      <tr>
        <td><%= link_to 'Show', meeting_room_reservation %></td>
        <td><%= link_to 'Edit', edit_management_portal_meeting_room_reservation_path(meeting_room_reservation) %></td>
        <td><%= link_to 'Destroy', meeting_room_reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @meeting_room_reservations %>

<br>

<%= link_to '予約を新規登録する', new_management_portal_meeting_room_reservation_path %>
<%= link_to 'TOPに戻る', management_portal_path %>
