<p id="notice"><%= notice %></p>

<h1>Management Portal Departments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @management_portal_departments.each do |management_portal_department| %>
      <tr>
        <td><%= link_to 'Show', management_portal_department %></td>
        <td><%= link_to 'Edit', edit_management_portal_department_path(management_portal_department) %></td>
        <td><%= link_to 'Destroy', management_portal_department, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Management Portal Department', new_management_portal_department_path %>
